@model TBtest.Models.Project
@{
    ViewBag.Title = "Edit Post";
}
<form asp-action="edit" asp-controller="home"  asp-route-id="@Model.ProjectId">
    <div class="form-group">
        <label asp-for="PrjName" class="control-label">PrjName</label>
        <input type="text" asp-for="PrjName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PrjDesc" class="control-label">PrjDesc</label>
        <input type="text" asp-for="PrjDesc" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PrjImg" class="control-label">PrjImg</label>
        <input type="text" asp-for="PrjImg" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PrjCreatedBy" class="control-label">PrjCreatedBy</label>
        <input type="number" asp-for="PrjCreatedBy" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PrjLikeCounter" class="control-label">PrjLikeCounter</label>
        <input type="number" asp-for="PrjLikeCounter" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Send" class="btn btn-default" />
    </div>
</form>